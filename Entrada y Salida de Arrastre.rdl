    <DataSet Name="indicador">
      <Query>
        <DataSourceName>Easybank</DataSourceName>
        <CommandText>WITH tempoTable AS (
	SELECT a.cuecta_numid,MIN(d.precuo_fecvenc) AS 'FechaVencimiento'
	, ISNULL(DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()),0) AS atraso
	, CASE WHEN ISNULL(datediff(day, MIN(d .precuo_fecvenc), getdate()),0) = 0 THEN '0 días' 
		WHEN datediff(day, MIN(d .precuo_fecvenc), getdate()) BETWEEN 1 AND 30 THEN '1-30 días' 
		WHEN datediff(day, MIN(d .precuo_fecvenc), getdate()) BETWEEN 31 AND 60 THEN '31-60 días' 
		WHEN datediff(day, MIN(d .precuo_fecvenc), getdate()) BETWEEN 61 AND 90 THEN '61-90 días' 
		WHEN datediff(day, MIN(d .precuo_fecvenc), getdate()) BETWEEN 91 AND 120 THEN '91-120 días' 
		WHEN datediff(day,MIN(d .precuo_fecvenc), getdate()) BETWEEN 121 AND 150 THEN '121-150 días' 
		WHEN datediff(day, MIN(d .precuo_fecvenc), getdate()) BETWEEN 151 AND 180 THEN '151-180 días' 
		WHEN datediff(day, MIN(d .precuo_fecvenc), getdate()) &gt;=181 THEN '181 + días'
		ELSE ' ' END AS 'Tramo Actual',MIN(d.precuo_fecvenc) AS fechevenc
	FROM epreprem a 
	LEFT JOIN eprecuom d ON a.cuecta_numid = d.cuecta_numid AND d.precuo_motivo = 1 AND d.admsts_codigo = 'A' 
	AND d.precuo_fecvenc &lt;= GETDATE() AND (d.precuo_salgas + d.precuo_salcom + d.precuo_salmor + d.precuo_salint + d.precuo_salcap) &gt; 0 
	WHERE  (a.admsts_codigo IN (4, 5)) 
	GROUP BY a.cuecta_numid
)

SELECT (SELECT COUNT(*) FROM tempoTable WHERE tempoTable.atraso &gt;91) AS 'Arrastre',
		(SELECT COUNT(*) FROM tempoTable WHERE tempoTable.atraso &gt;180) AS '&gt;181'</CommandText>
      </Query>
      <Fields>
        <Field Name="Arrastre">
          <DataField>Arrastre</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ID_181">
          <DataField>&gt;181</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
