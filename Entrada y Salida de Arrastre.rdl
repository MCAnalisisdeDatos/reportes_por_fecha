    <DataSet Name="Salieron_arrastre">
      <Query>
        <DataSourceName>Easybank</DataSourceName>
        <CommandText>WITH TramoAnteriorTable
     AS (
     SELECT esibpred.cuecta_numid,
            CASE
                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                THEN MAX(ed.sibcuo_dias) + 3
                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                THEN MAX(ed.sibcuo_dias) + 2
                ELSE MAX(ed.sibcuo_dias) + 1
            END AS 'Dias',
--,DATENAME(w,eh.Sibpre_fecha),MAX(ed.sibcuo_dias)
            CASE
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) = 0
                THEN '0 días'
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) BETWEEN 1 AND 30
                THEN '1-30 días'
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) BETWEEN 31 AND 60
                THEN '31-60 días'
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) BETWEEN 61 AND 90
                THEN '61-90 días'
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) BETWEEN 91 AND 120
                THEN '91-120 días'
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) BETWEEN 121 AND 150
                THEN '121-150 días'
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) BETWEEN 151 AND 180
                THEN '151-180 días'
                WHEN ISNULL(CASE
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Friday'
                                THEN MAX(ed.sibcuo_dias) + 3
                                WHEN DATENAME(w, eh.Sibpre_fecha) = 'Saturday'
                                THEN MAX(ed.sibcuo_dias) + 2
                                ELSE MAX(ed.sibcuo_dias) + 1
                            END, 0) &gt;= 181
                THEN '181 + días'
            END AS 'Tramo Anterior',
            SUM(ed.sibcuo_capital) AS 'Capital',
            SUM(ed.sibcuo_gastos) AS 'Gastos',
            SUM(ed.sibcuo_interes) AS 'Interes',
            SUM(ed.sibcuo_mora) AS 'Mora',
            SUM(ed.sibcuo_capital + ed.sibcuo_gastos + ed.sibcuo_gastos + ed.sibcuo_mora) AS 'Pendiente',
            esibpred.prepre_saldo AS 'Saldo'
     FROM esibpred
          LEFT JOIN esibpreh eh ON esibpred.Admpro_numid = eh.Admpro_numid
          LEFT JOIN esibcuod ed ON esibpred.admpro_numid = ed.admpro_numid
                                   AND esibpred.cuecta_numid = ed.cuecta_numid
     WHERE esibpred.admpro_numid =
(
    SELECT MAX(Admpro_numid)
    FROM esibpreh
    WHERE esibpreh.Sibpre_fecha NOT IN
(
    SELECT MAX(esibpreh.Sibpre_fecha)
    FROM esibpreh
)
)
           AND ed.precuo_numcuota IS NOT NULL
	--AND ed.cuecta_numid = 68018
	--AND ed.precuo_numid IN ()
     GROUP BY esibpred.cuecta_numid,
              esibpred.prepre_saldo,
              eh.Sibpre_fecha),
------------------------------------------------------------------------------------------------------------------------
     TramoActualTable
     AS (
     SELECT a.cuecta_numid,
            MIN(d.precuo_fecvenc) AS 'FechaVencimiento',
            ISNULL(DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()), 0) AS atraso,
            CASE
                WHEN ISNULL(DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()), 0) = 0
                THEN '0 días'
                WHEN DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()) BETWEEN 1 AND 30
                THEN '1-30 días'
                WHEN DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()) BETWEEN 31 AND 60
                THEN '31-60 días'
                WHEN DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()) BETWEEN 61 AND 90
                THEN '61-90 días'
                WHEN DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()) BETWEEN 91 AND 120
                THEN '91-120 días'
                WHEN DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()) BETWEEN 121 AND 150
                THEN '121-150 días'
                WHEN DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()) BETWEEN 151 AND 180
                THEN '151-180 días'
                WHEN DATEDIFF(day, MIN(d.precuo_fecvenc), GETDATE()) &gt;= 181
                THEN '181 + días'
                ELSE ' '
            END AS 'Tramo Actual',
            MIN(d.precuo_fecvenc) AS fechevenc,
            SUM(DISTINCT d.precuo_salcap) AS capital,
            SUM(DISTINCT d.precuo_salint) AS interes,
            SUM(DISTINCT d.precuo_salmor) AS Mora,
            SUM(DISTINCT d.precuo_salgas) AS Gastos,
            SUM(DISTINCT d.precuo_salcap) + SUM(DISTINCT d.precuo_salint) + SUM(DISTINCT d.precuo_salmor) + SUM(DISTINCT d.precuo_salgas) AS saldopendiente
     FROM epreprem a
          LEFT JOIN eprecuom d ON a.cuecta_numid = d.cuecta_numid
                                  AND d.precuo_motivo = 1
                                  AND d.admsts_codigo = 'A'
                                  AND d.precuo_fecvenc &lt;= GETDATE()
                                  AND (d.precuo_salgas + d.precuo_salcom + d.precuo_salmor + d.precuo_salint + d.precuo_salcap) &gt; 0 
							--WHERE a.cuecta_numid = 33048
     GROUP BY a.cuecta_numid),
	 -----------------------------------------------------------------------------------------------------------------
     cteDealer
     AS (
     SELECT pre.cuecta_numid,
            admgar_valor,
            admcga_lista
     FROM epreprem prem
          LEFT JOIN epreprer pre ON prem.cuecta_numid = pre.cuecta_numid
          LEFT JOIN eadmgarm garm ON pre.admgar_numero = garm.admgar_numero
          LEFT JOIN eadmginr ginr ON ginr.admgar_numero = garm.admgar_numero
          LEFT JOIN eadmcgam cgam ON cgam.admtin_codigo = ginr.admtin_codigo
          LEFT JOIN eadmcgar cgar ON cgar.admgar_numero = pre.admgar_numero
                                     AND cgar.admcga_id = cgam.admcga_id
     WHERE cgam.admcga_lista = 3),
	 ---------------------------------------------------------------------------------------------------------------
     CteTipoSeguro
     AS (
     SELECT a.cuecta_numid,
            REPLACE(REPLACE(ISNULL(
(
    SELECT DISTINCT
           CASE
               WHEN p.admgas_nombre IS NULL
               THEN 'SEGURO EXTERNO'
               WHEN p.admgas_nombre IS NOT NULL
               THEN LTRIM(RTRIM(p.admgas_nombre))
           END AS 'A'
    FROM eadmgasr AS o
         LEFT OUTER JOIN eadmgasm AS p ON o.admgas_codigo = p.admgas_codigo
    WHERE o.cuecta_numid = a.cuecta_numid
          AND o.admgas_codigo IN(213, 223, 535, 517, 113, 122, 239, 516, 703)
         AND o.admsts_codigo = 1 FOR XML PATH('')
), 'SEGURO EXTERNO'), '&lt;A&gt;', ''), '&lt;/A&gt;', '|') AS 'tipo'
     FROM epreprem a)
     --WHERE a.admsts_codigo IN(4, 5))
	 ---------------------------------------------------------------------------------------------------------------
     SELECT CAST(b.admcon_codigo AS CHAR(3))+' - '+i.admcon_nombre AS 'Producto',
            b.cuecta_formateado,
            a.cuecta_numid,
            a.cuecta_nombre,
            a.prepre_tasaint,
            MAX(a.prepre_montoaprob) AS 'Monto Aprobado',
            ant.Saldo AS Saldo,
            ant.[Tramo Anterior],
            act.[Tramo Actual],
            ant.Capital,
            ant.Interes,
            ant.Mora,
            ant.Gastos,
            ant.Pendiente,
            ant.Dias,
            DATEADD(DD, -1 * (ant.Dias + 1), GETDATE()) AS 'Fecha Vencimiento',
(
    SELECT MAX(ecuetrxm.cuetrx_fecha)
    FROM ecuetrxm
    WHERE ecuetrxm.cuecta_numid = a.cuecta_numid
          AND ecuetrxm.admctr_numid = 4
          AND ecuetrxm.admsts_codigo = 1
) AS 'ultimo Pago',
            LTRIM(RTRIM(ed.admsts_nombre)) AS 'Estado Préstamos',
            xad.admrel_nombre AS 'Promotor',
            relm.admrel_nombre AS 'Nombre Ejecutivo',
            cte.admgar_valor AS 'Dealer',
            tipoS.tipo AS 'Tipo'
     FROM epreprem AS a
          LEFT OUTER JOIN ecuectam AS b ON a.cuecta_numid = b.cuecta_numid
          LEFT JOIN xadmrelm xad ON xad.admrel_codigo = b.admpro_codigo
          LEFT JOIN xadmrelm relm ON relm.admrel_codigo = b.admeje_codigo
          LEFT JOIN cteDealer cte ON a.cuecta_numid = cte.cuecta_numid
          LEFT OUTER JOIN eadmconm AS i ON b.admcon_codigo = i.admcon_codigo
          LEFT JOIN TramoAnteriorTable AS ant ON a.cuecta_numid = ant.cuecta_numid
          LEFT JOIN TramoActualTable AS act ON a.cuecta_numid = act.cuecta_numid
          LEFT JOIN xadmstsm ed ON a.admsts_codigo = ed.admsts_codigo
                                   AND ed.admsts_tabla = 'epreprem'
          LEFT JOIN CteTipoSeguro tipoS ON a.cuecta_numid = tipoS.cuecta_numid
--WHERE        --(a.admsts_codigo IN (4, 5)) 
--AND
 --a.cuecta_numid =  67444
     GROUP BY b.cuecta_formateado,
              a.cuecta_numid,
              a.cuecta_nombre,
              a.admsts_codigo,
              a.prepre_diacorte,
              a.prepre_tasaint,
              ant.Saldo,
              b.admcon_codigo,
              i.admcon_nombre,
              act.atraso,
              ant.[Tramo Anterior],
              act.[Tramo Actual],
              ant.Capital,
              ant.Interes,
              ant.Mora,
              ant.Gastos,
              ant.Pendiente,
              ant.Dias,
              ed.admsts_nombre,
              xad.admrel_nombre,
              relm.admrel_nombre,
              cte.admgar_valor,
              tipoS.tipo
     HAVING act.atraso &lt; 92
            AND MAX(ant.Dias) &gt;= 92;


--select * from esibpreh ORDER BY  esibpreh.Sibpre_fecha DESC</CommandText>
